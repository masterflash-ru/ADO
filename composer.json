{
    "name": "masterflash-ru/ado",
    "description": "Аналог ADO от Microsoft",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "Zendframework",
        "ado",
        "laminas"
    ],
    "homepage": "https://www.masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2 || ^8.0",
        "laminas/laminas-hydrator": "*",
        "laminas/laminas-db": "*"
    },
    "autoload": {
        "psr-4": {
            "ADO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ADO\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-develop": "2.2-dev"
        },
        "zf": {
            "component": "ADO",
            "config-provider": "masterflash-ru\\ADO\\ConfigProvider"
        }
    }
}
