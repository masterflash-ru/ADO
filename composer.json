{
    "name": "masterflash-ru/Ado",
    "description": "Аналог ADO от Microsoft",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "Zendframework",
        "ado",
        "zf"
    ],
    "homepage": "http://www.masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-session": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "ADO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ADO\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "component": "ADO",
            "config-provider": "masterflash-ru\\ADO\\ConfigProvider"
        }
    }
}
